        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction rdf:nodeID="ub20bL334C69">
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="http://promsns.org/def/do#is_question_for_confirming">
                <rdfs:domain rdf:resource="http://promsns.org/def/do#Question"/>
                <rdfs:subPropertyOf rdf:resource="http://promsns.org/def/do#is_question_for"/>
                <rdfs:label xml:lang="en">is question for confirming</rdfs:label>
              </owl:ObjectProperty>
            </owl:onProperty>
            <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
          </owl:Restriction>
          <owl:Restriction rdf:nodeID="ub20bL338C69">
            <owl:allValuesFrom>
              <owl:Restriction rdf:nodeID="ub20bL340C89">
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
                <owl:onProperty rdf:resource="http://promsns.org/def/do#indicates_boolean_value"/>
              </owl:Restriction>
            </owl:allValuesFrom>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="http://promsns.org/def/do#has_answer">
                <rdfs:domain rdf:resource="http://promsns.org/def/do#Question"/>
                <owl:inverseOf rdf:resource="http://promsns.org/def/do#is_answer_for"/>
                <rdfs:label xml:lang="en">has answer</rdfs:label>
                <rdfs:range rdf:resource="http://promsns.org/def/do#Answer"/>
