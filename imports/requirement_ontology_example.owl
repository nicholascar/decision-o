                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="&requirement_ontology;Hazard"/>
                                    <owl:Class>
                                        <owl:complementOf>
                                            <owl:Restriction>
                                                <owl:onProperty rdf:resource="&requirement_ontology;is_described_by"/>
